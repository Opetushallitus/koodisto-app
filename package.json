{
    "homepage": "/koodisto-app",
    "name": "koodisto-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@opetushallitus/virkailija-ui-components": "^0.4.7",
        "@tanstack/match-sorter-utils": "^8.1.1",
        "@tanstack/react-table": "^8.5.11",
        "axios": "^0.26.0",
        "jotai": "^1.6.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "papaparse": "^5.3.2",
        "react": "^17.0.2",
        "react-accessible-accordion": "^4.0.0",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.31.3",
        "react-intl": "^6.6.6",
        "react-notifications-component": "^3.4.1",
        "react-papaparse": "^4.0.2",
        "react-router-dom": "^6.3.0",
        "reactjs-popup": "^2.0.5",
        "styled-components": "^5.3.3",
        "universal-cookie": "^4.0.4",
        "url": "^0.11.0"
    },
    "devDependencies": {
        "@formatjs/intl": "^2.10.2",
        "@iconify/icons-fa-solid": "^1.2.2",
        "@iconify/react": "^3.1.4",
        "@types/lodash": "^4.14.182",
        "@types/node": "^20.12.11",
        "@types/papaparse": "^5.3.2",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "@types/react-select": "3.0.14",
        "@types/react-table": "^7.7.9",
        "@types/styled-components": "^5.1.24",
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.8.0",
        "concurrently": "^7.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^6.11.0",
        "cypress": "^10",
        "cypress-file-upload": "^5.0.8",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-cypress": "^3.2.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.1",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "html-webpack-plugin": "^5.6.0",
        "mini-css-extract-plugin": "^2.9.0",
        "prettier": "^2.5.1",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.4.2",
        "tsx": "^4.10.2",
        "typescript": "^4.9.5",
        "wait-on": "^6.0.1",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-manifest-plugin": "^5.0.0"
    },
    "scripts": {
        "start": "NODE_ENV=development npx webpack serve",
        "mock-api": "npm --prefix ./mock-api run mock-api",
        "build": "GENERATE_SOURCEMAP=false NODE_ENV=production npx webpack --mode production",
        "test": "node --import tsx --test ./src/**/*.test.ts",
        "lint": "tsc --noEmit && eslint --ext .ts,.tsx src",
        "lint:fix": "eslint --ext .ts,.tsx src --fix",
        "prettier": "prettier --check 'src/**/*{js,jsx,ts,tsx}'",
        "prettier:fix": "prettier --write 'src/**/*{js,jsx,ts,tsx}'",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "cypress:ci": "START_SERVER_AND_TEST_INSECURE=1 concurrently  --success first --kill-others 'wait-on http://localhost:3000/koodisto-app && npm run cypress:run' 'npm run mock-api' 'npm run start'"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint",
            "cypress",
            "react"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended",
            "plugin:react/recommended",
            "plugin:cypress/recommended"
        ],
        "settings": {
            "react": {
                "version": "detect"
            }
        },
        "rules": {
            "@typescript-eslint/no-unused-vars": [
                "error",
                {
                    "ignoreRestSiblings": true
                }
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://localhost:9000",
    "engines": {
        "npm": "^10",
        "node": "^20"
    }
}
